/**index.wxss**/
.swz-tab-bar {
    width: 400rpx;
    height: 100rpx;
    background-color: #fff;
    border-radius: 200rpx;
    position: fixed;
    bottom: 80rpx;
    margin-left: 50%;
    transform: translate(-50%, 0);
    display: flex;
    flex-direction: row;
}

.swz-tab-bar-items {
    display: flex;
    flex-direction: column;
    width: 200rpx;
    height: 100rpx;
}

.swz-tab-bar-items-img {
    width: 50rpx;
    height: 50rpx;
    margin-left: 50%;
    transform: translate(-50%, 0);
    margin-top: 15rpx;
}

.swz-tab-bar-items-text {
    font-size: 20rpx;
    width: 100rpx;
    line-height: 20rpx;
    text-align: center;
    margin-left: 50%;
    transform: translate(-50%, 0);
    font-family: FZYS, serif;
}

@import 'index-wxa-auto-dark.wxss'
